<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/logo_sapne"
    tools:context="com.example.intents.sapne.our_volunteer">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Volunteer Head"
        android:textSize="35sp"
        android:background="#05d5f5"
        android:textColor="#f16101"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="29dp"
        android:layout_marginTop="7dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/vaibhav"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignLeft="@+id/button6"
        android:layout_alignStart="@+id/button6" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/nikita"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5" />

    <Button
        android:id="@+id/button5"
        android:layout_width="175sp"
        android:layout_height="43sp"
        android:text="Nikita Gupta ,
ADMIN/HR HEAD"
        android:textColor="#123456"
        android:background="#5afa03"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/imageView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button6"
        android:layout_width="175sp"
        android:layout_height="43sp"
        android:text="Vibhav ,
SOCIAL MEDIA HEAD"
        android:textColor="#123456"
        android:background="#5afa03"
        android:layout_above="@+id/imageView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/button7"
        android:layout_width="175sp"
        android:layout_height="43sp"
        android:text="Bhumika Sukhija,
CAMP COORDINATOR head"
        android:textColor="#123456"
        android:background="#5afa03"

        android:layout_below="@+id/imageView"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView" />

    <Button
        android:id="@+id/button8"
        android:layout_width="172sp"
        android:layout_height="43sp"
        android:text="Indraja Salunkhe,
GRAPHIC DESIGNER HEAD"
        android:textColor="#123456"
        android:background="#5afa03"
        android:layout_below="@+id/imageView2"
        android:layout_alignLeft="@+id/button6"
        android:layout_alignStart="@+id/button6" />

    <Button
        android:id="@+id/button9"
        android:onClick="vol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="view ALL Volunteers"
        android:textColor="#222222"
        android:background="#fc2f81"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bhumika"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/button5"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/indraja"
        android:layout_alignTop="@+id/imageView"
        android:layout_alignLeft="@+id/button8"
        android:layout_alignStart="@+id/button8"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp" />


</RelativeLayout>
