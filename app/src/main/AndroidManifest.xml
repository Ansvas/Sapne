<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.intents.sapne">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="Sapne"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label">
        <activity android:name=".BaseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ExpandingCells"
            android:noHistory="true" />
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Notifications" />
        <activity android:name=".JoinUs" />

        <activity android:name=".Register_user"
                  android:label="Sign up"
                  android:screenOrientation="portrait"
            />
        <activity android:name=".ProfileActivity"
                  android:label="Sapne User Information"
                  android:screenOrientation="portrait"
            />
        <activity android:name=".LoginActivity"
                  android:label="Sign in"
                  android:screenOrientation="portrait"
            >

        </activity>

        <activity
            android:name=".Activities"
            android:label="Activities Corner "
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Products"
            android:label="Products"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".NewFrag"
            android:noHistory="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RegularCamps"
            android:label="Regular camps "
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".our_volunteer"
            android:label="Our Team"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Contact_us"
            android:label="Contact Us "
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AboutUs"
            android:label="@string/about_us"
            android:noHistory="true"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".OurMission"
            android:label="@string/our_mission"
            android:noHistory="true"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Ring_contact_us"
            android:label="Contact Us"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".OurVision"
            android:label="@string/title_activity_our_vision"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Location_contact_us"
            android:label="Location"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".all_volunteer"
            android:label="Volunteers"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".OurTeam"
            android:label="@string/title_activity_our_team"
            android:noHistory="true"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EventsActivity"
            android:label="@string/title_activity_events"
            android:noHistory="true"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EventsExtraActivity"
            android:label="Events"
            android:noHistory="true"
            android:theme="@style/AppTheme" />
    </application>

</manifest>